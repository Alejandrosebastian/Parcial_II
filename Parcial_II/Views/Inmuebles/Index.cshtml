@model IEnumerable<Parcial_II.Models.Inmuebles>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.direccion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.N_habitaciones)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Costo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.activo)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Parroquia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Propietario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipos_inmu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N_habitaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Costo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.activo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parroquia.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Propietario.Apellido1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tipos_inmu.nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InmueblesId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InmueblesId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InmueblesId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
